   ],
   "source": [
    "plot(range(1, ordK + 1),  # Plot the planning modulation,\n",
    "     exp(matmul(C, array(M5.params[3::2]))), \n",
    "     label='Planning')\n",
    "plot(range(1, ordK + 1),  # ... and the movement modulation,\n",
    "     exp(matmul(C, array(M5.params[4::2]))), \n",
